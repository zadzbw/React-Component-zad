{
  "rules": {
    "indent": 0,
    "quotes": [
      2,
      "single"
    ],
    "no-console": 0,
    "no-unused-vars": 0,
    "semi": [
      2,
      "always"
    ],
    "eqeqeq": 2
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ]
}
