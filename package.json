{
  "name": "react-component-zad",
  "version": "1.0.0",
  "description": "a simple react component lib",
  "main": "src/components/index.js",
  "scripts": {
    "test": "jest -u",
    "start": "NODE_ENV=development webpack-dev-server --hot --inline --progress --colors --history-api-fallback",
    "build": "NODE_ENV=production webpack --progress --profile --colors",
    "deploy": "npm run build && DEBUG=deploy:* babel-node bin/deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zadzbw/React-Component-zad.git"
  },
  "keywords": [
    "react",
    "component"
  ],
  "author": "zad",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "clean-webpack-plugin": "^0.1.13",
    "css-loader": "^0.25.0",
    "debug": "^2.6.3",
    "enzyme": "^2.6.0",
    "enzyme-to-json": "^1.4.1",
    "eslint": "^3.7.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "jest": "^17.0.3",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "postcss-loader": "^0.13.0",
    "react-addons-test-utils": "^15.4.1",
    "react-transform-hmr": "^1.0.4",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/jest-script-transformer"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/enzyme/",
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-addons-test-utils"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "font-awesome": "^4.6.3",
    "lodash": "^4.16.4",
    "rc-animate": "^2.3.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "velocity-animate": "^1.4.0"
  },
  "bugs": {
    "url": "https://github.com/zadzbw/React-Component-zad/issues"
  },
  "homepage": "https://github.com/zadzbw/React-Component-zad#readme"
}
